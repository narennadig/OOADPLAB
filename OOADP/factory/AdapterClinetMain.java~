package pkgAdapter;

public class AdapterClinetMain {

	public static void main(String[] args) {
		Square oSq = new Square((float)12.5); 
		oSq.setColour("Magenta");oSq.setLineType("Dashed Line");
		oSq.displayArea(); oSq.drawShape();
		
		Rectangle oRect = new Rectangle((float)10.5, (float)4.5); 
		oRect.setColour("Peach");oRect.setLineType("Dotted Line");
		oRect.displayArea(); oRect.drawShape();
		
		Circle oCircle = new Circle((float)5.5,"Lavender","Dots & Dashes Line"); 
		//oCircle.setColour("Lavender"); oCircle.setLineType("Dots & Dashes Line");
		oCircle.displayArea(); oCircle.drawShape();

	}

}
